<div class="container">
  <div class="row">
    <div class="span8">
      Platforms
      <ul class="nav nav-pills">
        <li class="active"><a href="#">All</a></li>
        <% if (devicePlatforms) { %>
          <% devicePlatforms.forEach( function(platform){ %>
            <li><a href="#"><%- platform %></a></li>
          <% }); %>
        <% } %>
      </ul>
      Offices
      <ul class="nav nav-pills">
        <li class="active"><a href="#">All</a></li>
        <% if (deviceOffices) { %>
          <% deviceOffices.forEach( function(office){ %>
            <li><a href="#"><%- office %></a></li>
          <% }); %>
        <% } %>
      </ul>
      Availability
      <ul class="nav nav-pills">
        <li class="active"><a href="#">All</a></li>
        <li><a href="#">Checked In</a></li>
        <li><a href="#">Checked Out</a></li>
      </ul>
      <div id="map-container">
        <img id="hudl-east-1" class="east-1-map" src="../img/hudl-zones/hudl-east-1.png"></img>
        <img id="hudl-east-1-east" class="east-1-map inactive" src="../img/hudl-zones/hudl-east-1-east.png"></img>
        <img id="hudl-east-1-west" class="east-1-map inactive" src="../img/hudl-zones/hudl-east-1-west.png"></img>
        <img id="hudl-east-1-cart" class="east-1-map inactive" src="../img/hudl-zones/hudl-east-1-cart.png"></img>
      </div>
    </div>
    <div class="span4">
      <div id="devices-list">
      <% if (devicesByZone) { %>
        <% Array.prototype.forEach.call(Object.keys(devicesByZone), function(zone){ %>
          <h3><%- zone %></h3>
          <% if (devicesByZone[zone]) {%>
            <% Array.prototype.forEach.call(devicesByZone[zone], function(device){ %>
              <h4><%- device.name %></h4>
            <% }); %>
          <% } %>
        <% }); %>
      <% } %>
      </div>
    </div>
  </div>
</div>
